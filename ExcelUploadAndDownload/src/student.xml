<?xml version="1.0" encoding="UTF-8"?>
<excel id="student" code="student" name="学生信息录入">
	<colgroup>
		<col index="A" width='17em'></col>
		<col index="B" width='17em'></col>
		<col index="C" width='17em'></col>
		<col index="D" width='17em'></col>
		<col index="E" width='17em'></col>
		<col index="F" width='17em'></col>
	</colgroup>
	<title>
		<tr height="16px">
			<td rowspan="1" colspan="6" value="学生信息录入模板"></td>
		</tr>
	</title>
	<thead>
		<tr height="16px">
			<th value="编号"></th>
			<th value="姓名"></th>
			<th value="年龄"></th>
			<th value="性别"></th>
			<th value="出生日期"></th>
			<th value="爱好"></th>
		</tr>
	</thead>
	<tbody>
		<tr height="16px" firstrow="2" firstcol="0" repeat="5">
			<td type="string" isnullable="false" maxlength="30"></td>
			<td type="string" isnullable="false" maxlength="50"></td>
			<td type="numeric" format="##0" isnullable="false"></td>
			<td type="enum" format="男,女" isnullable="true"></td>
			<td type="date" isnullable="false" maxlength="30"></td>
			<td type="enum" format="足球,篮球,乒乓球" isnullable="true"></td>
		</tr>
	</tbody>
</excel>